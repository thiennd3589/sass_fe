    },\r\n    password: {\r\n      value: \"\",\r\n      errorMessage: \"\",\r\n      showError: false,\r\n    },\r\n    loginEmail: {\r\n      value: \"\",\r\n      errorMessage: \"\",\r\n      showError: false,\r\n    },\r\n    loginPassword: {\r\n      value: \"\",\r\n      errorMessage: \"\",\r\n      showError: false,\r\n    },\r\n    enableRedirect: false,\r\n  });\r\n\r\n  const { loginResult, signUpResult } = useSelector(\r\n    (state: State) => ({\r\n      loginResult: state.loginResult,\r\n      signUpResult: state.signUpResult,\r\n    }),\r\n    shallowEqual\r\n  );\r\n\r\n  useEffect(() => {\r\n    if (loginResult) {\r\n      if (loginResult.success) {\r\n        //Save Token\r\n        const loginResponse = loginResult.response as Obj;\r\n        const token = (loginResponse.data as Obj).accessToken as string;\r\n        Global.user.token = token;\r\n        Global.isAuthenticated = true;\r\n\r\n        localStorage.setItem(\"sassToken\", token);\r\n        //Redirect;\r\n        history.push(\"/\");\r\n        return;\r\n      } else {\r\n        setState((prevState) => ({\r\n          ...prevState,\r\n          email: {\r\n            ...prevState.email,\r\n            errorMessage: \"Your email may be incorrect\",\r\n            showError: true,\r\n          },\r\n          password: {\r\n            ...prevState.password,\r\n            errorMessage: \"Your password may be incorrect\",\r\n            showError: true,\r\n          },\r\n        }));\r\n      }\r\n    } else if (signUpResult) {\r\n      if (signUpResult.success) {\r\n        onLogin();\r\n        return;\r\n      } else {\r\n        setState((prevState) => ({\r\n          ...prevState,\r\n          email: {\r\n            ...prevState.email,\r\n            errorMessage: \"Your email may be incorrect\",\r\n            showError: true,\r\n          },\r\n          password: {\r\n            ...prevState.password,\r\n            errorMessage: \"Your password may be incorrect\",\r\n     